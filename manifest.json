{
  "name": "My Bookmarks",
  "version": "1.0",
  "description": "Bookmark manager allows for add, edit, view and delete bookmarks. Fetches thumnail of bookmark url",
  "permissions": [
    "bookmarks"
  ],
  "browser_action": {
      "default_title": "My Bookmarks.",
      "default_icon": "icon.png",
      "default_popup": "popup.html"
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'"
}
